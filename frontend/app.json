{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need permission to access your camera.",
        "NSMicrophoneUsageDescription": "We need microphone access for voice commands.",
        "NSSpeechRecognitionUsageDescription": "We need speech recognition to process voice commands.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "me.mohammedmisran.accessiblemedtracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "me.mohammedmisran.accessiblemedtracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "We need permission to access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "405f1110-1040-41ca-8023-3366d647a6e1"
      }
    }
  }
}
